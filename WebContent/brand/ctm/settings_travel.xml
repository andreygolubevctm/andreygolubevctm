<!-- NOTE: If a setting applies to ALL verticals put it in the settings_all<environment_name>.xml file or dragons will attack you. -->
<settings>
	<window-title>Compare the Market</window-title>
	<exit-url>http://int.comparethemarket.com.au/travel-insurance/</exit-url>
	<quote-url>ctm/travel.jsp</quote-url>
	<fsg-url>/ctm/legal/FSG.pdf</fsg-url>
	<vertical>TRAVEL</vertical>
	<send>
		<sendUrl>../../dreammail/send.jsp</sendUrl>
		<sendUrlEnv>_QAA</sendUrlEnv>
		<sendYN>Y</sendYN>
		<app>
			<MailingName>RTM_CTM_TRAVEL_APP</MailingName>
			<tmpl>travel_app</tmpl>
		</app>
		<edm>
			<MailingName>RTM_CTM_TRAVEL_QUOTE</MailingName>
			<tmpl>travel_edm</tmpl>
			<xSQL>Y</xSQL>
		</edm>
		<quote>
			<MailingName>RTM_CTM_TRAVEL_QUOTE</MailingName>
			<tmpl>travel_quote</tmpl>
		</quote>
	</send>
</settings>