<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:include schemaLocation="../life/contactDetails.xsd"/>
	<xs:include schemaLocation="../shared/components.xsd"/>
	<xs:include schemaLocation="../shared/address.xsd"/>
	<xs:element name="ip">
		<xs:complexType>
			<xs:all>
				<xs:element name="primary">
					<xs:complexType>
						<xs:all>
							<xs:element name="insurance" >
								<xs:complexType>
									<xs:all>
										<xs:element name="incomeentry"  maxOccurs="1" minOccurs="0" />
										<xs:element name="income"  maxOccurs="1" minOccurs="1" />
										<!-- TODO upgrade to xsd 1.1 so we can compare income > amount -->
										<xs:element name="amountentry"  maxOccurs="1" minOccurs="0" />
										<xs:element name="amount" maxOccurs="1" minOccurs="1" />
										<xs:element name="value" maxOccurs="1" minOccurs="1" />
										<xs:element name="waiting" maxOccurs="1" minOccurs="1" />
										<xs:element name="benefit" maxOccurs="1" minOccurs="1" />
										<xs:element name="type" maxOccurs="1" minOccurs="0" />
										<xs:element name="partner" type="YN" maxOccurs="1" minOccurs="0" />
										<xs:element name="frequency"  maxOccurs="1" minOccurs="0" />
									</xs:all>
								</xs:complexType>
							</xs:element>
							<xs:element name="firstName" type="nonEmptyString" maxOccurs="1" minOccurs="1" />
							<xs:element name="lastname" type="nonEmptyString" maxOccurs="1" minOccurs="1" />
							<xs:element name="gender" type="gender" maxOccurs="1" minOccurs="1" />
							<xs:element name="dob" type="date" maxOccurs="1" minOccurs="1" />
							<xs:element name="age" type="age" maxOccurs="1" minOccurs="0" />
							<xs:element name="smoker" maxOccurs="1" minOccurs="1" />
							<xs:element name="postCode" type="postcode" maxOccurs="1" minOccurs="1" />
							<xs:element name="state" type="state" maxOccurs="1" minOccurs="0" />
							<xs:element name="occupation" type="fourtyCharHashCode" maxOccurs="1" minOccurs="1" />
							<xs:element name="productid" maxOccurs="1" minOccurs="0" />
						</xs:all>
					</xs:complexType>
				</xs:element>
				<xs:element name="partner" maxOccurs="1" minOccurs="0" type="noValidation" />
				<!-- no need to validate -->
				<xs:element name="splitTestingJourney" maxOccurs="1" minOccurs="0" />
				<xs:element name="contactLeadSent" maxOccurs="1" minOccurs="0" />
				<xs:element name="contactDetails" type="contactDetails" />
				<xs:element name="transactionId" maxOccurs="1" minOccurs="0" />
				<xs:element name="operatorid" maxOccurs="1" minOccurs="0" />
				<xs:element name="client" maxOccurs="1" minOccurs="0" />
				<xs:element name="clientIpAddress" maxOccurs="1" minOccurs="0" />
				<xs:element name="clientUserAgent" maxOccurs="1" minOccurs="0" />
				<xs:element name="api" maxOccurs="1" minOccurs="0" />
				<xs:element name="refine" maxOccurs="1" minOccurs="0" type="noValidation" />
				<xs:element name="tracking" maxOccurs="1" minOccurs="0" />
				<xs:element name="privacyoptin" maxOccurs="1" minOccurs="0" />
			</xs:all>
		</xs:complexType>
	</xs:element>
</xs:schema>