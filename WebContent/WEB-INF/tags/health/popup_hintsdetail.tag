			$('#hints-detail-dialog').dialog("open");
		}
		else
		{
			FatalErrorDialog.exec({
				message:		"Requested content doesn't exist.",
				page:			"health:popup_hintsdetail.tag",
				description:	"HintsDetailsDialog.launch(). Could not find element with ID: #hd-" + id,
				data:			null
			});
		}
	},
	
	hide: function()
	{
		$("#hints-detail-dialog").hide();
	},

	show: function()
	{
		$("#hints-detail-dialog").show();
	}
};

HintsDetailDialog.addContent


</go:script>
