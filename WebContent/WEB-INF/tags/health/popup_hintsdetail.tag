			},
			close: function(){
				HintsDetailDialog.hide();	
		  	}
		});	
	},
	
	launch: function( id )
	{
		if( $('#hints-detail-dialog').dialog("isOpen") )
		{
			$('#hints-detail-dialog').dialog("close");
		}
		
		if( $('#hd-' + id) )
		{
			$('#hints-detail-dialog').find(".innertube").first().empty().append(
