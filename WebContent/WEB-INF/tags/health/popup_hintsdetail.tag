<go:script marker="js-head">
var HintsDetailDialog = new Object();
HintsDetailDialog = {

	init: function()
	{
		// Initialise the search quotes dialog box
		// =======================================
		$('#hints-detail-dialog').dialog({
			autoOpen: false,
			show: 'clip',
			hide: 'clip', 
			'modal':true, 
			'width':637, 'height':'auto', 
			'minWidth':637/*, 'minHeight':715*/,  
			'autoOpen': false,
			'draggable':false,
			'resizable':false,
			'dialogClass':'hints-detail-dialog',
			'title':'Hint Dialog Title.',
