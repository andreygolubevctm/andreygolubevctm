<?xml version="1.0" encoding="UTF-8"?>
<aggregator xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xsi:schemaLocation="http://com.disc_au.soap.aggregator ../schema/config.xsd"
			xmlns="http://com.disc_au.soap.aggregator">

	<merge-root>soap-response</merge-root>
	<merge-xsl>merge-results.xsl</merge-xsl>
	<config-dir>/aggregator/avea_payment</config-dir>
	<error-dir>/usr/aih/app-logs/ctm/WEB-INF/aggregator/avea_payment/errors</error-dir>
	<debug-dir>/usr/aih/app-logs/ctm/WEB-INF/aggregator/avea_payment/debug</debug-dir>

	<service name="AVEA" type="soap">
		<soap-url>https://staging.avea.com.au:2001/PaymentService.svc</soap-url>
		<soap-action>http://services.fastr.com.au/Payment/IPaymentService/ProcessAndUpdateMainInsured</soap-action>
		<outbound-xsl>AVEA_outbound_payment.xsl</outbound-xsl>
		<inbound-xsl>AVEA_inbound_payment.xsl</inbound-xsl>
		<timeoutMillis>20000</timeoutMillis>
	</service>

</aggregator>
