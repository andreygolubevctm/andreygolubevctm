<?xml version="1.0" encoding="UTF-8"?>
<aggregator xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xsi:schemaLocation="http://com.disc_au.soap.aggregator ../schema/config.xsd"
			xmlns="http://com.disc_au.soap.aggregator">

	<merge-root>soap-response</merge-root>
	<merge-xsl>merge-getretailerplans.xsl</merge-xsl>
	<config-dir>aggregator/utilities</config-dir>
	<error-dir>/usr/aih/app-logs/ctm/WEB-INF/aggregator/utilities/errors</error-dir>
	<debug-dir>/usr/aih/app-logs/ctm/WEB-INF/aggregator/utilities/debug</debug-dir>

	<service name="GETELECTRICITYPLANS" type="SOAP">
		<soap-url>https://websvc.switchwise.com.au:444/SwitchwiseCTM_1_5_8/SwitchwiseSearchService.svc/RetailerPlans/</soap-url>
		<soap-user>ctm</soap-user>
		<soap-password>Ctm#138s</soap-password>
		<soap-method>POST</soap-method>
		<outbound-xsl>utilities_getretailerplans_outbound.xsl</outbound-xsl>
		<outbound-xsl-parms>packagetype=Electricity,classtype=Electricity</outbound-xsl-parms>
		<inbound-xsl>utilities_getretailerplans_inbound.xsl</inbound-xsl>
		<inbound-xsl-parms>packagetype=Electricity,classtype=Electricity</inbound-xsl-parms>
		<timeoutMillis>20000</timeoutMillis>
	</service>

	<service name="GETGASPLANS" type="SOAP">
		<soap-url>https://websvc.switchwise.com.au:444/SwitchwiseCTM_1_5_8/SwitchwiseSearchService.svc/RetailerPlans/</soap-url>
		<soap-user>ctm</soap-user>
		<soap-password>Ctm#138s</soap-password>
		<soap-method>POST</soap-method>
		<outbound-xsl>utilities_getretailerplans_outbound.xsl</outbound-xsl>
		<outbound-xsl-parms>packagetype=Gas,classtype=Gas</outbound-xsl-parms>
		<inbound-xsl>utilities_getretailerplans_inbound.xsl</inbound-xsl>
		<inbound-xsl-parms>packagetype=Gas,classtype=Gas</inbound-xsl-parms>
		<timeoutMillis>20000</timeoutMillis>
	</service>

</aggregator>
