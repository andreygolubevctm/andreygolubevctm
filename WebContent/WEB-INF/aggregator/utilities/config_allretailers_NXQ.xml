<?xml version="1.0" encoding="UTF-8"?>
<aggregator xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xsi:schemaLocation="http://com.disc_au.soap.aggregator ../schema/config.xsd"
			xmlns="http://com.disc_au.soap.aggregator">

	<merge-root>soap-response</merge-root>
	<merge-xsl>merge-allretailers.xsl</merge-xsl>
	<config-dir>aggregator/utilities</config-dir>
	<error-dir>/usr/aih/app-logs/ctm/WEB-INF/aggregator/utilities/errors</error-dir>
	<debug-dir>/usr/aih/app-logs/ctm/WEB-INF/aggregator/utilities/debug</debug-dir>

	<service name="ALLRETAILERS" type="SOAP">
		<soap-url>http://websvcpreprod.switchwise.com.au/switchwise1_5_10/SwitchwiseSearchService.svc/AllRetailers</soap-url>
		<soap-method>GET</soap-method>
		<outbound-xsl>utilities_allretailers_outbound.xsl</outbound-xsl>
		<outbound-xsl-parms></outbound-xsl-parms>
		<inbound-xsl>utilities_allretailers_inbound.xsl</inbound-xsl>
		<inbound-xsl-parms>defaultProductId=NODEFAULT,service=DEFAULT</inbound-xsl-parms>
		<timeoutMillis>20000</timeoutMillis>
	</service>

</aggregator>
