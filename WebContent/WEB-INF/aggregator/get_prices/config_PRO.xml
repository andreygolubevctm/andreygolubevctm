<?xml version="1.0" encoding="UTF-8"?>
<aggregator xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
			xsi:schemaLocation="https://com.disc_au.soap.aggregator ../schema/config.xsd"
			xmlns="https://com.disc_au.soap.aggregator">

	<merge-root>soap-response</merge-root>
	<merge-xsl>merge-results.xsl</merge-xsl>
	<config-dir>aggregator/get_prices</config-dir>
	<error-dir>/usr/aih/app-logs/ctm/WEB-INF/aggregator/get_prices/errors</error-dir>
	<debug-dir>/usr/aih/app-logs/ctm/WEB-INF/aggregator/get_prices/debug</debug-dir>

	<service name="AGIS_BUDD" type="soap">
		<soap-url>https://ecommerce.disconline.com.au/services/2.0/getCarQuotes</soap-url>
		<soap-action>getCarQuotes</soap-action>
		<outbound-xsl>AGIS_outbound.xsl</outbound-xsl>
		<outbound-xsl-parms>partnerId=CTM0000002,sourceId=0000000001</outbound-xsl-parms>
		<inbound-xsl>AGIS_inbound.xsl</inbound-xsl>
		<inbound-xsl-parms>defaultProductId=BUDD-05-01,service=AGIS_BUDD,certServer=secure.</inbound-xsl-parms>
		<timeoutMillis>20000</timeoutMillis>
	</service>

	<service name="AGIS_OTH1" type="soap">
		<soap-url>https://ecommerce.disconline.com.au/services/2.0/getCarQuotes</soap-url>
		<soap-action>getCarQuotes</soap-action>
		<outbound-xsl>AGIS_outbound.xsl</outbound-xsl>
		<outbound-xsl-parms>partnerId=CTM0000002,sourceId=0000000002</outbound-xsl-parms>
		<inbound-xsl>AGIS_inbound.xsl</inbound-xsl>
		<inbound-xsl-parms>defaultProductId=OZIC-05-01,service=AGIS_OZIC,certServer=secure.</inbound-xsl-parms>
		<timeoutMillis>20000</timeoutMillis>
	</service>

	<service name="REIN" type="url-encoded">
		<soap-url>https://ecommerce.disconline.com.au/ctm/rating/car/car_price_service_REIN.jsp</soap-url>
		<outbound-xsl>REIN_copy_nodes.xsl</outbound-xsl>
		<inbound-xsl>REIN_copy_nodes.xsl</inbound-xsl>
		<inbound-xsl-parms>productId=REIN-01-02,urlRoot=https://quote.realinsurance.com.au/quotelines/car/referral/comparethemarket/</inbound-xsl-parms>
		<timeoutMillis>41000</timeoutMillis>
	</service>

	<service name="AI" type="soap">
		<soap-url>https://b2b.aiinsurance.com.au/SSGateway/SSGateway.asmx</soap-url>
		<soap-action>http://www.softsure.co.za/GetVehiclePremium</soap-action>
		<ssl-no-host-verify>Y</ssl-no-host-verify>
		<outbound-xsl>AI_outbound.xsl</outbound-xsl>
		<inbound-xsl>AI_inbound.xsl</inbound-xsl>
		<inbound-xsl-parms>productId=AI-01-01,quoteURL=https://b2b.aiinsurance.com.au/AIOnlineBuy/Buy/KnockOutQuestions/</inbound-xsl-parms>
		<timeoutMillis>20000</timeoutMillis>
	</service>

	<service name="CRSR" type="soap">
		<soap-url>https://services.avea.com.au:8080/QuotationService.svc</soap-url>
		<soap-action>http://services.fastr.com.au/Quotation/IQuotationService/RequestInitialQuotation</soap-action>
		<outbound-xsl>AVEA_outbound.xsl</outbound-xsl>
		<outbound-xsl-parms>servicePassword=captaincompare</outbound-xsl-parms>
		<inbound-xsl>CRSR_inbound.xsl</inbound-xsl>
		<inbound-xsl-parms>productId=CRSR-01-01</inbound-xsl-parms>
		<timeoutMillis>20000</timeoutMillis>
	</service>

	<service name="WOOL" type="url-encoded">
		<soap-url>https://ecommerce.disconline.com.au/ctm/rating/car/car_price_service_WOOL.jsp</soap-url>
		<outbound-xsl>WOOL_copy_nodes.xsl</outbound-xsl>
		<inbound-xsl>WOOL_copy_nodes.xsl</inbound-xsl>
		<inbound-xsl-parms>productId=WOOL-01-02</inbound-xsl-parms>
		<timeoutMillis>30000</timeoutMillis>
	</service>

</aggregator>
