<?xml version="1.0" encoding="UTF-8"?>
<aggregator xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xsi:schemaLocation="http://com.disc_au.soap.aggregator ../schema/config.xsd"
			xmlns="http://com.disc_au.soap.aggregator">

	<merge-root>soap-response</merge-root>
	<merge-xsl>merge-results.xsl</merge-xsl>
	<config-dir>/aggregator/health_application</config-dir>
	<error-dir>/usr/aih/app-logs/ctm/WEB-INF/aggregator/health_application/errors</error-dir>
	<debug-dir>/usr/aih/app-logs/ctm/WEB-INF/aggregator/health_application/debug</debug-dir>

	<service name="WFD" type="soap">
		<soap-url>http://localhost:8080/ctm/ajax/write/health_submit_wfd.jsp</soap-url>
		<soap-action>Submit</soap-action>

		<outbound-xsl>auf/outbound.xsl</outbound-xsl>
		<inbound-xsl>wfd/inbound.xsl</inbound-xsl>
		<timeoutMillis>120000</timeoutMillis>
	</service>

</aggregator>