		<outbound-xsl-parms>partnerId=CTM0000100,sourceId=0000000001,schemaVersion=3.0</outbound-xsl-parms>
		<inbound-xsl>AGIS/inbound_results.xsl</inbound-xsl>
		<inbound-xsl-parms>defaultProductId=BUDD-05-29,service=AGIS_BUDD,certServer=secure.</inbound-xsl-parms>
