<?xml version="1.0" encoding="UTF-8"?>
<aggregator xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xsi:schemaLocation="http://com.disc_au.soap.aggregator ../schema/config.xsd"
			xmlns="http://com.disc_au.soap.aggregator">

	<merge-root>soap-response</merge-root>
	<config-dir>aggregator/home</config-dir>
	<error-dir>/usr/aih/app-logs/ctm/WEB-INF/aggregator/home/errors</error-dir>
	<debug-dir>home/debug-rein</debug-dir>

	<service name="REINCONTENT" type="soap">
		<soap-url>https://everyonepp.realinsurance.com.au/v2/AggregatorService.svc</soap-url>
		<soap-action>http://tempuri.org/IAggregatorService/GetContentForToken</soap-action>
		<outbound-xsl>Hollard/content_outbound.xsl</outbound-xsl>
		<outbound-xsl-parms>partnerId=CTM0000001,sourceId=TEST000001</outbound-xsl-parms>
		<inbound-xsl>Hollard/content_inbound.xsl</inbound-xsl>
		<inbound-xsl-parms>productId=REIN-02-01,service=REIN</inbound-xsl-parms>
		<timeoutMillis>35000</timeoutMillis>
	</service>

</aggregator>
