		<outbound-xsl>DUIN_outbound.xsl</outbound-xsl>
		<outbound-xsl-parms>partnerId=CC00000001,sourceId=TEST000001</outbound-xsl-parms>
		<inbound-xsl>DUIN_inbound.xsl</inbound-xsl>
		<inbound-xsl-parms>defaultProductId=NODEFAULT,service=DUIN</inbound-xsl-parms>
