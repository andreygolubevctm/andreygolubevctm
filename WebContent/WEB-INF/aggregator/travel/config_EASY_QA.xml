<?xml version="1.0" encoding="UTF-8"?>
<aggregator xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"			
			xsi:schemaLocation="http://com.disc_au.soap.aggregator ../schema/config.xsd"
			xmlns="http://com.disc_au.soap.aggregator">
		
	<merge-root>soap-response</merge-root>	
	<merge-xsl>merge-results.xsl</merge-xsl>
	<config-dir>/WEB-INF/aggregator/travel</config-dir>
	<error-dir>/WEB-INF/aggregator/travel/errors</error-dir>
	<debug-dir>/WEB-INF/aggregator/travel/debug</debug-dir>
			
	<service name="EASY" type="url-encoded">
		<soap-url>http://qa.ecommerce.disconline.com.au/cc/rating/travel_price_service_EASY.jsp</soap-url>
		<outbound-xsl>EASY_outbound.xsl</outbound-xsl>
		<outbound-xsl-parms>partnerId=CC00000001,sourceId=TEST000001</outbound-xsl-parms>
		<inbound-xsl>EASY_inbound.xsl</inbound-xsl>
		<inbound-xsl-parms>defaultProductId=NODEFAULT,service=EASY</inbound-xsl-parms>
		<timeoutMillis>20000</timeoutMillis>
