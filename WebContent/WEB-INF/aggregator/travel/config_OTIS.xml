<?xml version="1.0" encoding="UTF-8"?>
<aggregator xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xsi:schemaLocation="http://com.disc_au.soap.aggregator ../schema/config.xsd"
			xmlns="http://com.disc_au.soap.aggregator">

	<merge-root>soap-response</merge-root>
	<merge-xsl>merge-results.xsl</merge-xsl>
	<config-dir>aggregator/travel</config-dir>
	<error-dir>aggregator/travel/app-logs-errors</error-dir>
	<debug-dir>aggregator/travel/app-logs-debug</debug-dir>

	<service name="OTIS" type="url-encoded">
		<soap-url>http://localhost:8080/cc/rating/travel_price_service_OTIS.jsp</soap-url>
		<outbound-xsl>OTIS_outbound.xsl</outbound-xsl>
		<outbound-xsl-parms>partnerId=CC00000001,sourceId=TEST000001</outbound-xsl-parms>
		<inbound-xsl>OTIS_inbound.xsl</inbound-xsl>
		<inbound-xsl-parms>defaultProductId=NODEFAULT,service=OTIS</inbound-xsl-parms>
		<timeoutMillis>20000</timeoutMillis>
	</service>

</aggregator>
