	<xsd:simpleType name="claimTypeDamage">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="TD"/> <!-- Attempted Theft Dmg -->
			<xsd:enumeration value="MD"/> <!-- Malicious Damage -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="claimTypeAccident">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AD"/> <!-- Accident Damage -->
			<xsd:enumeration value="OT"/> <!-- Other -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="reasonForNoInsurance">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="01"/> <!-- Just Purchased Used From Dealer - Still At Dealer -->
			<xsd:enumeration value="02"/> <!-- Just Purchased Used From Dealer - Taken From Dealer -->
