			<xsd:enumeration value="04"/> <!-- Extended Travel or Illness -->
			<xsd:enumeration value="05"/> <!-- Car Never Used Before -->
			<xsd:enumeration value="06"/> <!-- Couldn't Afford Insurance -->
			<xsd:enumeration value="07"/> <!-- Car Has Not Been Driveable -->
			<xsd:enumeration value="08"/> <!-- Insurance Cancelled or Refused By Insurance Co. -->
			<xsd:enumeration value="09"/> <!-- Just Purchased New - Still at Dealer -->
			<xsd:enumeration value="10"/> <!-- Just Purchased New - Taken From Dealer -->
			<xsd:enumeration value="11"/> <!-- Insurance Not Renewed In Time -->
			<xsd:enumeration value="12"/> <!-- Insurance Cancelled By Policy Holder -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="marketAgreedValue">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="M"/> <!-- Market Value -->
			<xsd:enumeration value="A"/> <!-- Agreed Value -->
