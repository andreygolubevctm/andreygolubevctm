<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns="https://ecommerce.disconline.com.au/services/schema/3.1/codes"
	targetNamespace="https://ecommerce.disconline.com.au/services/schema/3.1/codes"
	elementFormDefault="unqualified">

	<xsd:simpleType name="yesNo">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Y"/> <!-- Yes -->
			<xsd:enumeration value="N"/> <!-- No -->
		</xsd:restriction>

	</xsd:simpleType>

	<xsd:simpleType name="yesNoDontKnow">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Y"/> <!-- Yes -->
			<xsd:enumeration value="N"/> <!-- No -->
			<xsd:enumeration value="U"/> <!-- Don't Know -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="yes">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Y"/> <!-- Yes -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="no">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="N"/> <!-- Yes -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="empty">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value=""/> <!-- Empty String -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="gender">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="M"/> <!-- Male -->
			<xsd:enumeration value="F"/> <!-- Female -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="cover">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="A"/> <!-- Full Comprehensive -->
			<xsd:enumeration value="B"/> <!-- Third Party, Fire and Theft-->
			<xsd:enumeration value="C"/> <!-- Third Party Only -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="headlineOffer">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ONLINE"/> <!-- onlineQuote is headline offer -->
			<xsd:enumeration value="OFFLINE"/> <!-- offlineQuote is headline offer -->
			<xsd:enumeration value=""/> <!-- No headline offer -->
		</xsd:restriction>
	</xsd:simpleType>

	<!-- Composite type of all ncd rating classifications -->
	<xsd:simpleType name="ncdRating">
		<xsd:union memberTypes="noNcdRating midNcdRating maxNcdRating">
		</xsd:union>
	</xsd:simpleType>

	<xsd:simpleType name="accessoryCode">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="7"/>
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="noNcdRating">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="0"/> <!-- Zero (Rating 6) -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="midNcdRating">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="1"/> <!-- 1 Year (Rating 5) -->
			<xsd:enumeration value="2"/> <!-- 2 Years (Rating 4) -->
			<xsd:enumeration value="3"/> <!-- 3 Years (Rating 3) -->
			<xsd:enumeration value="4"/> <!-- 4 Years (Rating 2) -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="maxNcdRating">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="5"/> <!-- 5 Years (Rating 1) -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="ageRestriction">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="3"/> <!-- No - I want anybody to drive the car -->
			<xsd:enumeration value="H"/> <!-- Yes - I am happy for all drivers to be 21 and over -->
			<xsd:enumeration value="4"/> <!-- Yes - I am happy for all drivers to be 25 and over -->
			<xsd:enumeration value="7"/> <!-- Yes - I am happy for all drivers to be 25 and over -->
			<xsd:enumeration value="A"/> <!-- Yes - I am happy for all drivers to be 30 and over -->
			<xsd:enumeration value="D"/> <!-- Yes - I am happy for all drivers to be 40 and over -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="parkingLocation">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="1"/> <!-- Garaged -->
			<xsd:enumeration value="2"/> <!-- Street -->
			<xsd:enumeration value="3"/> <!-- Driveway -->
			<xsd:enumeration value="4"/> <!-- On Private Property -->
			<xsd:enumeration value="5"/> <!-- Car Park -->
			<xsd:enumeration value="6"/> <!-- Lot -->
			<xsd:enumeration value="7"/> <!-- Locked Compound -->
			<xsd:enumeration value="8"/> <!-- Carport -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="maritalStatus">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="A"/> <!-- Separated -->
			<xsd:enumeration value="D"/> <!-- Divorced -->
			<xsd:enumeration value="M"/> <!-- Married -->
			<xsd:enumeration value="P"/> <!-- Cohabiting/Partnered -->
			<xsd:enumeration value="S"/> <!-- Single -->
			<xsd:enumeration value="W"/> <!-- Widowed -->
			<xsd:enumeration value="U"/> <!-- Unknown -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="vehicleUse">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="02"/> <!-- Private and Commuting to work -->
			<xsd:enumeration value="11"/> <!-- Private and Occasional Business -->
			<xsd:enumeration value="12"/> <!-- Private and Business -->
			<xsd:enumeration value="13"/> <!-- Goods Carrying for Reward -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="state">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="ACT"/> <!-- Australian Capital Territory -->
			<xsd:enumeration value="NSW"/> <!-- New South Wales -->
			<xsd:enumeration value="NT"/> <!-- Northern Territory -->
			<xsd:enumeration value="QLD"/> <!-- Queensland -->
			<xsd:enumeration value="SA"/> <!-- South Australia -->
			<xsd:enumeration value="TAS"/> <!-- Tasmania -->
			<xsd:enumeration value="VIC"/> <!-- Victoria -->
			<xsd:enumeration value="WA"/> <!-- Western Australia -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="finance">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="HP"/> <!-- Finance/Hire Purchase -->
			<xsd:enumeration value="LE"/> <!-- Lease -->
			<xsd:enumeration value="NO"/> <!-- None -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="employmentStatus">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="E"/> <!-- Employed - Full Time -->
			<xsd:enumeration value="P"/> <!-- Employed - Part Time -->
			<xsd:enumeration value="S"/> <!-- Self-Employed -->
			<xsd:enumeration value="R"/> <!-- Retired -->
			<xsd:enumeration value="H"/> <!-- Home duties -->
			<xsd:enumeration value="F"/> <!-- Student -->
			<xsd:enumeration value="U"/> <!-- Unemployed -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="suspensionReason">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AP"/> <!-- Accumulated demerit points -->
			<xsd:enumeration value="SO"/> <!-- Single offence - alcohol, drugs, dangerous driving -->
			<xsd:enumeration value="CC"/> <!-- Conviction - alcohol, drugs, dangerous driving -->
			<xsd:enumeration value="ST"/> <!-- Single offence - other -->
			<xsd:enumeration value="FD"/> <!-- Fine default only -->
			<xsd:enumeration value="MD"/> <!-- Medical reasons -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="relationship">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="S"/> <!-- Spouse -->
			<xsd:enumeration value="W"/> <!-- Cohabitee/Partner -->
			<xsd:enumeration value="OD"/> <!-- Daughter -->
			<xsd:enumeration value="OS"/> <!-- Son -->
			<xsd:enumeration value="M"/> <!-- Parent -->
			<xsd:enumeration value="F"/> <!-- Other Family -->
			<xsd:enumeration value="E"/> <!-- Employee -->
			<xsd:enumeration value="B"/> <!-- Employer -->
			<xsd:enumeration value="C"/> <!-- Business Partner -->
			<xsd:enumeration value="U"/> <!-- Unrelated -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="suspensionLength">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="1"/> <!-- Up to 1 month -->
			<xsd:enumeration value="3"/> <!-- Up to 3 month -->
			<xsd:enumeration value="6"/> <!-- Up to 6 month -->
			<xsd:enumeration value="12"/> <!-- Up to 12 month -->
			<xsd:enumeration value="18"/> <!-- More than 12 months -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="drivingConviction">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AL"/> <!-- Alcohol -->
			<xsd:enumeration value="DD"/> <!-- Dangerous Driving -->
			<xsd:enumeration value="DR"/> <!-- Drugs -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="claimType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="AD"/> <!-- Accident Damage -->
			<xsd:enumeration value="SC"/> <!-- Stolen Vehicle -->
			<xsd:enumeration value="TD"/> <!-- Attempted Theft Dmg -->
			<xsd:enumeration value="MD"/> <!-- Malicious Damage -->
			<xsd:enumeration value="OR"/> <!-- Riot -->
			<xsd:enumeration value="FD"/> <!-- Fire Damage -->
			<xsd:enumeration value="OS"/> <!-- Storm Damage -->
			<xsd:enumeration value="OL"/> <!-- Lightning Damage -->
			<xsd:enumeration value="OX"/> <!-- Explosion -->
			<xsd:enumeration value="WD"/> <!-- Window Damage/Loss -->
			<xsd:enumeration value="OT"/> <!-- Other -->
		</xsd:restriction>
	</xsd:simpleType>

	<xsd:simpleType name="claimTypeBasic">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="SC"/> <!-- Stolen Vehicle -->
			<xsd:enumeration value="OR"/> <!-- Riot -->
